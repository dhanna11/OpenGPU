# !/bin/bash

export LD_LIBRARY_PATH=$PWD/../mesa/lib
export LIBGL_DRIVERS_PATH=$PWD/../mesa/lib
export LIBGL_DRIVERS_PATH=$PWD/../mesa/lib/gallium
export LIBGL_ALWAYS_SOFTWARE=1
export GALLIUM_PRINT_OPTIONS=1

./openglapp_fixquad
./openglapp_tri
./openglapp_quad
./openglapp_cube
./openglapp_gears

exit 0